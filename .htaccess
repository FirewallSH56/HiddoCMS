RewriteEngine On

# Quickregister Rules
ErrorDocument 400 /error.php
ErrorDocument 401 /error.php
ErrorDocument 403 /error.php
ErrorDocument 404 /error.php
ErrorDocument 500 /error.php

RewriteRule ^index(|/)$ ./index.php
RewriteRule ^login(|/)$ ./login.php
RewriteRule ^register(|/)$ ./register.php
RewriteRule ^support(|/)$ ./support.php
RewriteRule ^news/(..*)$ ./news.php?url=$1
RewriteRule ^account/(..*)$ ./account.php?item=$1
RewriteRule ^photos/(..*)$ ./photos.php?url=$1
RewriteRule ^home/(..*)$ ./home.php?idd=$1
RewriteRule ^logout(|/)$ ./logout.php
RewriteRule ^banned(|/)$ ./banned.php
RewriteRule ^mantenimiento(|/)$ ./mantenimiento.php
RewriteRule ^disconnected(|/)$ ./disconnected.php
RewriteRule ^hotel(|/)$ ./hotel.php
RewriteRule ^me(|/)$ ./me.php
RewriteRule ^articles(|/)$ ./articles.php
RewriteRule ^articles/gratis(|/)$ ./articles-gratis.php
RewriteRule ^articles/hotel(|/)$ ./articles-hotel.php
RewriteRule ^articles/mobis(|/)$ ./articles-mobis.php
RewriteRule ^articles/promocao(|/)$ ./articles-promocao.php
RewriteRule ^articles/sistema(|/)$ ./articles-sistema.php
RewriteRule ^gallery(|/)$ ./gallery.php
RewriteRule ^famous(|/)$ ./famous.php
RewriteRule ^team(|/)$ ./team.php
RewriteRule ^shop(|/)$ ./shop.php